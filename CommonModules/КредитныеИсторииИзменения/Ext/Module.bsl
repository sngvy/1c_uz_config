				Результат.Добавить(Новый Структура("ИмяТаблицы, Поле, НБКИЮЛ, НаименованиеПоляРегистраКИ, ЗаполнятьПоУмолчанию, Значение, Функция, СтрокаДляПолученияЗначения, КодДопСвойства,
				| ОбъектДопСвойства, СтрокаХранилище","СубъектЮридическоеЛицо", Элемент.Поле, Элемент.НомерПоказателяНБКИЮЛ, Элемент.НаименованиеПоляРегистраКИ, Элемент.ЗаполнятьПоУмолчанию, Элемент.Значение, Элемент.Функция,
				Элемент.СтрокаДляПолученияЗначения, Элемент.КодДопСвойства, Элемент.ОбъектДопСвойства, Элемент.СтрокаХранилище));	
			КонецЦикла;	
			
			мсЗначения = Новый Массив;
			Для Каждого ТекСтрокаПодстановки Из Результат Цикл 
				ТекНастройка = Новый Структура("Поле, НБКИЮЛ, НаименованиеПоляРегистраКИ, ЗаполнятьПоУмолчанию, Значение",
				ТекСтрокаПодстановки.Поле, ТекСтрокаПодстановки.НБКИЮЛ, ТекСтрокаПодстановки.НаименованиеПоляРегистраКИ, ТекСтрокаПодстановки.ЗаполнятьПоУмолчанию); 
				
				Значение = УЭДСервер.ГенерацияЗначениия(ТекСтрокаПодстановки, Ложь, ВыборкаДетальныеЗаписи.Обязательство, ВыборкаДетальныеЗаписи.Обязательство);
				ФункцияОпределена = УЭДСервер.ФункцияУспешноОпределена(ТекСтрокаПодстановки);
				Если ФункцияОпределена Тогда
					Значение = УЭДСервер.ШаблоныТекстаДляАвтоинформирования_ВычислитьФункцию(ТекСтрокаПодстановки.Функция, НоваяЗапись.Должник, Значение);
					Если ТипЗнч(Значение) <> Тип("ТаблицаЗначений") Тогда
						Значение = СокрЛП(Значение);
					КонецЕсли;	
					ТекНастройка.Значение = Значение;
					мсЗначения.Добавить(ТекНастройка);
				Иначе 	
					Значение = СокрЛП(Значение);
					ТекНастройка.Значение = Значение;
					мсЗначения.Добавить(ТекНастройка);
				КонецЕсли; 
			КонецЦикла;	  
			
			МассивАдрес = Новый Массив;
			Для Каждого структура Из мсЗначения Цикл
				
				Если ЗначениеЗаполнено(Структура.НаименованиеПоляРегистраКИ) Тогда
					НаименованиеПоляРегистраКИ = Структура.НаименованиеПоляРегистраКИ;
					ЗначениеВРегистр = Структура.Значение;
					//Если Структура.ЗаполнятьПоУмолчанию Тогда
					//	ЗначениеВРегистр = ВычислитьПараметрПоУмолчанию(Структура.НБКИФЛ, ВыборкаДетальныеЗаписи, НоваяЗапись.Сделка);
					//КонецЕсли;
					ЗначениеВРегистр = КредитныеИстории.ОбработатьЗначенияПризнаков(ЗначениеВРегистр); 
					НоваяЗапись[НаименованиеПоляРегистраКИ] = ЗначениеВРегистр;				
				ИначеЕсли СтрНайти(Структура.НБКИЮЛ, "B2.") > 0 Тогда
					МассивАдрес.Добавить(Структура); 
					Продолжить;
				Иначе
					Продолжить;
				КонецЕсли;
				
			КонецЦикла;	
			НоваяЗапись.Записать();
			НоваяЗапись = Неопределено;   
			
			
			Если ЗначениеЗаполнено(ВыборкаДетальныеЗаписи.Дееспособность)  Тогда
				ДокОбъект = ВыборкаДетальныеЗаписи.Дееспособность.ПолучитьОбъект();
				ДокОбъект.Записать();
			КонецЕсли;   
			
			КредитныеИстории.ЗаписатьАдресОбъекта(МассивАдрес, ВыборкаДетальныеЗаписи.Должник, ЭтоФизЛицо, Настройка.СтроковыеАдреса);
			
		КонецЕсли;		
	КонецЦикла;  
		
КонецПроцедуры

Процедура ДобавитьКТелуОтчетаОбъединениеКИ(тТелоОтчета, ФИОПредыдущее, ФИОТекущее, КоличествоГруппБлоков, Запись, ДатаОтчета, Настройка) Экспорт 
	// 6.01 Объединение двух кредитных историй одного субъекта 
	//ЮЛ/ФЛ, 
	
	Операция =  КредитныеИсторииИзменения.ОбработатьЗначениеПеречисленияОперацийКорректировки(Перечисления.ВидыОперацийИсправленияИУдаленияRUTDF.C_2);
	СобытиеКод = Справочники.КредитныеИсторииСобытияИсключенияАннулированияКорректировки.НайтиПоРеквизиту("КодНБКИ", "3.1").КодНБКИ;
	СтруктураПоляБлока = СформироватьСтрокуКорректировкиОбъединениеКИ(ФИОТекущее, ФИОПредыдущее);
	КредитныеИстории.СформироватьТекстГруппыGROUPHEADER(КоличествоГруппБлоков, тТелоОтчета, Операция, ДатаОтчета, СобытиеКод, "", "", "");
	КредитныеИстории.СформироватьБлокиТитульныхЗаписей(Запись, тТелоОтчета, Настройка);
	КредитныеИстории.ДобавитьКТелуОтчета(тТелоОтчета, "DELETE"); 
	СтрокаКорректировки = КредитныеИсторииИзменения.СформироватьСтрокуИсправлениеТитульныхЗаписейДолжника(Запись, "C1", СтруктураПоляБлока.ПоляБлока, СтруктураПоляБлока.СтрокаНаКорректировку);
	КредитныеИстории.ДобавитьКТелуОтчета(тТелоОтчета, СтрокаКорректировки);	
КонецПроцедуры

Функция СформироватьСтрокуКорректировкиОбъединениеКИ(ФИОТекущее, ФИОПредыдущее) Экспорт 
	// 6.01 Объединение двух кредитных историй одного субъекта
	СтруктураКорректировки = Новый Структура("ПоляБлока, СтрокаНаКорректировку");   
	стрПоляБлока = "";
	стрСтрокаНаКорректировку = "";
	Если Врег(ФИОТекущее.Фамилия) <> Врег(ФИОПредыдущее.Фамилия) Тогда
		стрПоляБлока = "1 ";
		стрСтрокаНаКорректировку = ФИОПредыдущее.Фамилия + " "; 
	КонецЕсли;	
	Если Врег(ФИОТекущее.Имя) <> Врег(ФИОПредыдущее.Имя) Тогда
		стрПоляБлока = стрПоляБлока + "2 ";
		стрСтрокаНаКорректировку = стрСтрокаНаКорректировку + ФИОПредыдущее.Имя + " ";  
	КонецЕсли;	
	Если Врег(ФИОТекущее.Отчество) <> Врег(ФИОПредыдущее.Отчество) Тогда 
		стрПоляБлока = стрПоляБлока + "3 ";
		стрСтрокаНаКорректировку = стрСтрокаНаКорректировку + ФИОПредыдущее.Отчество + " "; 
	КонецЕсли;
	стрПоляБлока = СокрЛП(стрПоляБлока);
	стрСтрокаНаКорректировку = СокрЛП(стрСтрокаНаКорректировку);
	
	стрПоляБлока = СтрЗаменить(стрПоляБлока, " ", "~");
	стрСтрокаНаКорректировку = СтрЗаменить(стрСтрокаНаКорректировку, " ", "~"); 
	
	СтруктураКорректировки.ПоляБлока = стрПоляБлока;
	СтруктураКорректировки.СтрокаНаКорректировку = стрСтрокаНаКорректировку;
			
	Возврат СтруктураКорректировки; 		
КонецФункции

           
