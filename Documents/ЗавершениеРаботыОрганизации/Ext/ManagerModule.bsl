
Процедура ИнициализироватьДанныеДокумента(ЭтотОбъект, ТаблицыДвижений) Экспорт
	
	Запрос = Новый Запрос();	
	Запрос.УстановитьПараметр("Ссылка", ЭтотОбъект.Ссылка);
	Запрос.УстановитьПараметр("Период",	ЭтотОбъект.Ссылка.МоментВремени());
	Запрос.УстановитьПараметр("Дата",	ЭтотОбъект.Дата);
	Запрос.УстановитьПараметр("Автор",	ЭтотОбъект.Автор);
	Запрос.УстановитьПараметр("Организация", ЭтотОбъект.Организация);
	
	ИнициализироватьДанныеОбъектыСобытия(
		Запрос,
		ТаблицыДвижений["Объекты"],
		ТаблицыДвижений["События"]
	);
	
	ИнициализироватьДанныеОтветственные(
		Запрос,
		ТаблицыДвижений["Ответственные"]
	);
	
	
	//ТаблицаДвиженийКатегории = ТаблицыДвижений["Категории"];
	//ТаблицаДвиженийЗадолженность = ТаблицыДвижений["Задолженность"];
	//ТаблицаДвиженийЗадолженностьВнесудебная = ТаблицыДвижений["ЗадолженностьВнесудебна"];
	//ТаблицаДвиженийЗадолженностьСудебная = ТаблицыДвижений["ЗадолженностьСудебная"];
	//ТаблицаДвиженийЗадолженностьИск = ТаблицыДвижений["ЗадолженностьИск"];
	//
	Если ЭтотОбъект.ОбнулитьКатегории ИЛИ ЭтотОбъект.ОбнулитьЗадолженность Тогда
		Запрос.УстановитьПараметр("Объекты", ЭтотОбъект.Объекты.Выгрузить().ВыгрузитьКолонку("Объект"));
		
	КонецЕсли;
	
	Если ЭтотОбъект.ОбнулитьКатегории Тогда
		Запрос.Текст = "ВЫБРАТЬ
		               |	&Дата КАК Период,
		               |	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
		               |	СостоянияПоКатегориямОстатки.Скоринг,
		               |	СостоянияПоКатегориямОстатки.Категория,
		               |	СостоянияПоКатегориямОстатки.Объект,
		               |	СостоянияПоКатегориямОстатки.КоличествоОстаток КАК Количество
		               |ИЗ
		               |	РегистрНакопления.СостоянияПоКатегориям.Остатки(&Период, Объект В (&Объекты)) КАК СостоянияПоКатегориямОстатки";	
		ТаблицаДвиженийКатегории = Запрос.Выполнить().Выгрузить();
	КонецЕсли;
	
	Если ЭтотОбъект.ОбнулитьЗадолженность Тогда
		Запрос.Текст = "ВЫБРАТЬ
		|	&Дата КАК Период,
		|	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
		|	ЗадолженностьПоОбъектамОстатки.Объект КАК Объект,
		|	ЗадолженностьПоОбъектамОстатки.СуммаДООстаток КАК СуммаДО,
		|	ЗадолженностьПоОбъектамОстатки.СуммаРеглОстаток КАК СуммаРегл,
		|	ЗадолженностьПоОбъектамОстатки.ОсновнойДолгДООстаток КАК ОсновнойДолгДО,
		|	ЗадолженностьПоОбъектамОстатки.ОсновнойДолгРеглОстаток КАК ОсновнойДолгРегл,
		|	ЗадолженностьПоОбъектамОстатки.ПроцентыДООстаток КАК ПроцентыДО,
		|	ЗадолженностьПоОбъектамОстатки.ПроцентыРеглОстаток КАК ПроцентыРегл,
		|	ЗадолженностьПоОбъектамОстатки.ШтрафыРеглОстаток КАК ШтрафыРегл,
		|	ЗадолженностьПоОбъектамОстатки.ШтрафыДООстаток КАК ШтрафыДО,
		|	ЗадолженностьПоОбъектамОстатки.ПениДООстаток КАК ПениДО,
		|	ЗадолженностьПоОбъектамОстатки.ПениРеглОстаток КАК ПениРегл,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая1ДООстаток КАК Составляющая1ДО,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая1РеглОстаток КАК Составляющая1Регл,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая2ДООстаток КАК Составляющая2ДО,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая2РеглОстаток КАК Составляющая2Регл,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая3ДООстаток КАК Составляющая3ДО,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая3РеглОстаток КАК Составляющая3Регл,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая4ДООстаток КАК Составляющая4ДООстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая4РеглОстаток КАК Составляющая4РеглОстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая5ДООстаток КАК Составляющая5ДООстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая5РеглОстаток КАК Составляющая5РеглОстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая6ДООстаток КАК Составляющая6ДООстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая6РеглОстаток КАК Составляющая6РеглОстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая7ДООстаток КАК Составляющая7ДООстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая7РеглОстаток КАК Составляющая7РеглОстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая8ДООстаток КАК Составляющая8ДООстаток,
		|	ЗадолженностьПоОбъектамОстатки.Составляющая8РеглОстаток КАК Составляющая8РеглОстаток
		|ИЗ
		|	РегистрНакопления.ЗадолженностьПоОбъектам.Остатки(&Период, Объект В (&Объекты)) КАК ЗадолженностьПоОбъектамОстатки";	
		ТаблицаДвиженийЗадолженность = Запрос.Выполнить().Выгрузить();
		
		Запрос.Текст = "ВЫБРАТЬ
		|	&Дата КАК Период,
		|	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Объект КАК Объект,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.СуммаДООстаток КАК СуммаДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.СуммаРеглОстаток КАК СуммаРегл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ОсновнойДолгДООстаток КАК ОсновнойДолгДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ОсновнойДолгРеглОстаток КАК ОсновнойДолгРегл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ПроцентыДООстаток КАК ПроцентыДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ПроцентыРеглОстаток КАК ПроцентыРегл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ШтрафыДООстаток КАК ШтрафыДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ШтрафыРеглОстаток КАК ШтрафыРегл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ПениДООстаток КАК ПениДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.ПениРеглОстаток КАК ПениРегл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая1ДООстаток КАК Составляющая1ДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая1РеглОстаток КАК Составляющая1Регл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая2ДООстаток КАК Составляющая2ДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая2РеглОстаток КАК Составляющая2Регл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая3ДООстаток КАК Составляющая3ДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая3РеглОстаток КАК Составляющая3Регл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая4ДООстаток КАК Составляющая4ДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая4РеглОстаток КАК Составляющая4Регл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая5ДООстаток КАК Составляющая5ДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая5РеглОстаток КАК Составляющая5Регл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая6ДООстаток КАК Составляющая6ДОк,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая6РеглОстаток КАК Составляющая6Регл,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая7ДООстаток КАК Составляющая7ДОк,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая7РеглОстаток КАК Составляющая7Реглк,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая8ДООстаток КАК Составляющая8ДО,
		|	ЗадолженностьПоОбъектамВнесудебнаяОстатки.Составляющая8РеглОстаток КАК Составляющая8Регл
		|ИЗ
		|	РегистрНакопления.ЗадолженностьПоОбъектамВнесудебная.Остатки(&Период, Объект В (&Объекты)) КАК ЗадолженностьПоОбъектамВнесудебнаяОстатки";
		ТаблицаДвиженийЗадолженностьВнесудебная = Запрос.Выполнить().Выгрузить();
		
		Запрос.Текст = "ВЫБРАТЬ
		|	&Дата КАК Период,
		|	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
		|	ЗадолженностьПоСудебнымРешениямОстатки.Займ КАК Займ,
		|	ЗадолженностьПоСудебнымРешениямОстатки.Решение КАК Решение,
		|	ЗадолженностьПоСудебнымРешениямОстатки.ЧастьРешения КАК ЧастьРешения,
		|	ЗадолженностьПоСудебнымРешениямОстатки.ТипЗадолженности КАК ТипЗадолженности,
		|	ЗадолженностьПоСудебнымРешениямОстатки.СуммаОстаток КАК Сумма
		|ИЗ
		|	РегистрНакопления.ЗадолженностьПоСудебнымРешениям.Остатки(&Период, Займ В (&Объекты)) КАК ЗадолженностьПоСудебнымРешениямОстатки"; 
		ТаблицаДвиженийЗадолженностьСудебная = Запрос.Выполнить().Выгрузить();
		
		Запрос.Текст = "ВЫБРАТЬ
		|	&Дата КАК Период,
		|	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
		|	ИсковыеТребованияОстатки.Займ КАК Займ,
		|	ИсковыеТребованияОстатки.СуммаОсновнойДолгОстаток КАК СуммаОсновнойДолг,
		|	ИсковыеТребованияОстатки.СуммаПроцентыОстаток КАК СуммаПроценты,
		|	ИсковыеТребованияОстатки.СуммаШтрафыОстаток КАК СуммаШтрафы,
		|	ИсковыеТребованияОстатки.СуммаПениОстаток КАК СуммаПени,
		|	ИсковыеТребованияОстатки.ЦенаИскаОстаток КАК ЦенаИска,
		|	ИсковыеТребованияОстатки.СуммаГоспошлинаОстаток КАК СуммаГоспошлина
		|ИЗ
		|	РегистрНакопления.ИсковыеТребования.Остатки(&Период, Займ В (&Объекты)) КАК ИсковыеТребованияОстатки";		
		ТаблицаДвиженийЗадолженностьИск = Запрос.Выполнить().Выгрузить();
	КонецЕсли;
	
	ТаблицыДвижений["Категории"] = ТаблицаДвиженийКатегории;
	ТаблицыДвижений["Задолженность"] = ТаблицаДвиженийЗадолженность;
	ТаблицыДвижений["ЗадолженностьВнесудебна"] = ТаблицаДвиженийЗадолженностьВнесудебная;
	ТаблицыДвижений["ЗадолженностьСудебная"] = ТаблицаДвиженийЗадолженностьСудебная;
	ТаблицыДвижений["ЗадолженностьИск"] = ТаблицаДвиженийЗадолженностьИск;

КонецПроцедуры

Процедура ИнициализироватьДанныеОбъектыСобытия(Запрос, ТаблицаДвиженийОбъекты, ТаблицаДвиженийСобытия)

	Запрос.Текст = "ВЫБРАТЬ
	               |	ЗавершениеРаботыОрганизацииОбъекты.Объект,
	               |	ЗавершениеРаботыОрганизацииОбъекты.Ссылка КАК Регистратор,
	               |	ЕСТЬNULL(ОбъектыВРаботеОстатки.КоличествоОстаток, 0) КАК ОстатокВРаботе,
	               |	ОбъектыВРаботеОстатки.Сотрудник,
	               |	ОбъектыВРаботеОстатки.Организация,
	               |	ОбъектыВРаботеОстатки.Подразделение
	               |ПОМЕСТИТЬ Таблица
	               |ИЗ
	               |	Документ.ЗавершениеРаботыОрганизации.Объекты КАК ЗавершениеРаботыОрганизацииОбъекты
	               |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОбъектыВРаботе.Остатки(&Период, Организация = &Организация) КАК ОбъектыВРаботеОстатки
	               |		ПО ЗавершениеРаботыОрганизацииОбъекты.Объект = ОбъектыВРаботеОстатки.Объект
	               |ГДЕ
	               |	ЗавершениеРаботыОрганизацииОбъекты.Ссылка = &Ссылка
	               |
	               |СГРУППИРОВАТЬ ПО
	               |	ЗавершениеРаботыОрганизацииОбъекты.Ссылка,
	               |	ОбъектыВРаботеОстатки.Организация,
	               |	ОбъектыВРаботеОстатки.Подразделение,
	               |	ЗавершениеРаботыОрганизацииОбъекты.Объект,
	               |	ОбъектыВРаботеОстатки.Сотрудник,
	               |	ЕСТЬNULL(ОбъектыВРаботеОстатки.КоличествоОстаток, 0)
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	&Дата КАК Период,
	               |	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
	               |	Таблица.Сотрудник,
	               |	Таблица.Объект,
	               |	Таблица.ОстатокВРаботе КАК Количество,
	               |	Таблица.ОстатокВРаботе КАК ОстатокВРаботе,
	               |	Таблица.Организация,
	               |	Таблица.Подразделение
	               |ИЗ
	               |	Таблица КАК Таблица
	               |
	               |УПОРЯДОЧИТЬ ПО
	               |	ОстатокВРаботе
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	&Дата КАК Период,
	               |	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
	               |	ВЫБОР
	               |		КОГДА Таблица.Сотрудник = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
	               |			ТОГДА ВЫБОР
	               |					КОГДА Таблица.Подразделение = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
	               |						ТОГДА ЗНАЧЕНИЕ(Перечисление.ВидыСобытий.ЗавершенаРаботаОрганизации)
	               |					ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ВидыСобытий.ЗавершенаРаботаПодразделения)
	               |				КОНЕЦ
	               |		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ВидыСобытий.ЗавершенаРаботаСотрудника)
	               |	КОНЕЦ КАК ВидСобытия,
	               |	Таблица.Регистратор КАК ИсточникСобытия,
	               |	Таблица.Объект,
	               |	Таблица.Сотрудник КАК Сотрудник,
	               |	&Автор,
	               |	Таблица.Подразделение КАК Подразделение
	               |ИЗ
	               |	Таблица КАК Таблица
	               |
	               |УПОРЯДОЧИТЬ ПО
	               |	Подразделение УБЫВ,
	               |	Сотрудник УБЫВ
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |УНИЧТОЖИТЬ Таблица";				   				   
	Результат = Запрос.ВыполнитьПакет();
	ТаблицаДвиженийОбъекты = Результат[1].Выгрузить();
	ТаблицаДвиженийСобытия = Результат[2].Выгрузить();

КонецПроцедуры

Процедура ИнициализироватьДанныеОтветственные(Запрос, ТаблицаДвиженийОтветственные)

	Запрос.Текст = "ВЫБРАТЬ
	               |	ЗавершениеРаботыОрганизацииОбъекты.Объект КАК Объект
	               |ПОМЕСТИТЬ ВтОбъектыДляЗавершения
	               |ИЗ
	               |	Документ.ЗавершениеРаботыОрганизации.Объекты КАК ЗавершениеРаботыОрганизацииОбъекты
	               |ГДЕ
	               |	ЗавершениеРаботыОрганизацииОбъекты.Ссылка = &Ссылка
	               |
	               |ИНДЕКСИРОВАТЬ ПО
	               |	Объект
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	ОтветственныеСотрудники.ТипСотрудника КАК ТипСотрудника,
	               |	ОтветственныеСотрудники.Пользователь КАК Пользователь,
	               |	""Ответственные"" КАК ТипРегистра,
	               |	ОтветственныеСотрудники.Объект КАК Объект
