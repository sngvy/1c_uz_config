			ДатаСтартаИсточника = ДатаРУТДФ;
		КонецЕсли;	
		ОчиститьСобытия30Дней(стрДоговор.ОбъектУчета, ?(ЗначениеЗаполнено(стрДоговор.ДатаСтартаВосстановления), стрДоговор.ДатаСтартаВосстановления, ДатаСтартаИсточника)); 
		
