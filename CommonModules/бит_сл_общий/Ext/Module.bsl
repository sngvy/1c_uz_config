// Возвращает локализированную строку
//
Функция ПолучитьСтрокуСообщения(ИдентификаторСтроки, Параметры = Неопределено, КодЯзыка = Неопределено) Экспорт
	
	СтрокаРезультат = "";
	ТребуетсяПодставновкаПараметров = Ложь;
	
	Если Не бит_сл_ЗначениеОпределено(КодЯзыка) Тогда		
		КодЯзыка = бит_сл_сервер_ассистент.ПолучитьЯзыкКлиентаЛицензирования();		
	КонецЕсли; 
	
	Локализация = ПолучитьДанныеЛокализации(КодЯзыка).СообщенияИСтроки;
	
	Если бит_сл_ЗначениеОпределено(Параметры) И
		 Тип(Параметры) = Тип("Соответствие") Тогда
		
		Если Параметры.Количество() > 0 Тогда 			
			ТребуетсяПодставновкаПараметров = Истина;  			
		КонецЕсли; 
		
	КонецЕсли;		
	
	ИдентификаторСтрокиНайден = Локализация.Свойство(ИдентификаторСтроки);
	
	Если ИдентификаторСтрокиНайден Тогда
		
		СтрокаРезультат = Локализация[ИдентификаторСтроки];
		
		Если ТребуетсяПодставновкаПараметров Тогда
			
			Для Каждого ЭлементЗн Из Параметры Цикл				
				СтрокаРезультат = СтрЗаменить(СтрокаРезультат, ЭлементЗн.Ключ, ЭлементЗн.Значение);				
			КонецЦикла;
			
		КонецЕсли;	
		
	Иначе
		
		СтрокаРезультат = "#текст локализации """ + ИдентификаторСтроки + """ не найден#";		 
		
	КонецЕсли; 
	
	Возврат СтрокаРезультат;
	
КонецФункции 

// Обходит строку, находит символы разделителя и формирует массив строк
// ДополнительныеНастройкиСтруктура:
//		МассивИсключения = массив с строками, элементы которых не будут возвращены.
// 		МассивИсключенияНеУчитыватьРегистр = булево, если установлено, не будет учитываться регистр при сравнении строк
Функция РазобратьСтрокуСРазделителямиВМассив(СтрокаСРазделителями, Разделитель, ИгнорироватьПробелыВокругСлова = Истина, ДополнительныеНастройкиСтруктура = Неопределено) Экспорт
	
	ВсеОк = Истина;
	МассивСтрок = Новый Массив;
	
	Если СтрокаСРазделителями = "" Тогда
		
		ВсеОк = Ложь;
		
	КонецЕсли;
	
	Если ВсеОк Тогда
		
		Если Разделитель <> ""  Тогда
			
			Буфер = "";
			
			Для итератор = 1 По СтрДлина(СтрокаСРазделителями) Цикл
				
				Символ = Сред(СтрокаСРазделителями, итератор, 1);
				
				Если Символ = Разделитель ИЛИ
					итератор = СтрДлина(СтрокаСРазделителями) Тогда
					
					Если итератор = СтрДлина(СтрокаСРазделителями) Тогда
						
						Буфер = Буфер + Символ;
						
					КонецЕсли;  
					
					Если Буфер <> "" Тогда
						
						ДобавляемаяСтрока = ?(ИгнорироватьПробелыВокругСлова, СокрЛП(Буфер), Буфер);
						Буфер = "";
						МассивСтрок.Добавить(ДобавляемаяСтрока);
						
					КонецЕсли; 
					
				Иначе
					
					Буфер = Буфер + Символ;
					
				КонецЕсли; 	
				
			КонецЦикла;		
			
		Иначе
			
			МассивСтрок.Добавить(СтрокаСРазделителями);		
			
		КонецЕсли; 	
		
	КонецЕсли; 
	
	// Применение доп. параметров
	Если ДополнительныеНастройкиСтруктура <> Неопределено Тогда
		
		// Применение массива исключения
		МассивИсключения = Новый Массив;		
		БезУчетаРегистра = ДополнительныеНастройкиСтруктура.МассивИсключенияНеУчитыватьРегистр;
		Для Каждого ЭлементЗн Из ДополнительныеНастройкиСтруктура.МассивИсключения Цикл
			
			МассивИсключения.Добавить( ?(БезУчетаРегистра, ВРег(ЭлементЗн), ЭлементЗн) );
			
		КонецЦикла;  
		
		ФильтрованныйМассивСтрок = Новый Массив;		
		Для Каждого ЭлементЗн Из МассивСтрок Цикл
			
			Если МассивИсключения.Найти( ?(БезУчетаРегистра, ВРег(ЭлементЗн), ЭлементЗн) ) = Неопределено Тогда
				
				ФильтрованныйМассивСтрок.Добавить(ЭлементЗн);				
				
			КонецЕсли; 
			
		КонецЦикла; 
		
		МассивСтрок = ФильтрованныйМассивСтрок;
		
	КонецЕсли; 

	Возврат МассивСтрок;
	
КонецФункции

#КонецОбласти