		Если РезультатЗапроса.Пустой() Тогда
			Продолжить;
		Иначе 
			Выгрузка = РезультатЗапроса.Выгрузить();
		КонецЕсли;	
		
		
		ДатаПредыдущегоСобытия = Выгрузка[0].ДатаСобытия;
		
		Сч = 0;
		
