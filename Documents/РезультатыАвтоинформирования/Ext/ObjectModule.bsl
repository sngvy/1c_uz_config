
Процедура ОбработкаЗаполнения(ДанныеЗаполнения, СтандартнаяОбработка)
	
	Если ТипЗнч(ДанныеЗаполнения) = Тип("ДокументСсылка.ЗаданияДляАвтоинформирования") Тогда
		НЗ = РегистрыНакопления.СостоянияПоАвтоинформированию.СоздатьНаборЗаписей();
		НЗ.Отбор.Регистратор.Установить(ДанныеЗаполнения);
		НЗ.Прочитать();
		
		Для Каждого Стр Из НЗ Цикл
			НС = Результаты.Добавить();
			НС.УИД = Стр.УИД;
		КонецЦикла;
	КонецЕсли;
	
КонецПроцедуры
